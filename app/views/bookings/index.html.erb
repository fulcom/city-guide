<h1>Here are all your bookings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <!-- <th>Banner url</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.id %></td>
        <td><%= booking.start_date %> to <%= booking.end_date %></td>
        <td><%= link_to 'Show' %></td>
      </tr>
    <% end %>
  </tbody>
</table>


